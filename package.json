{
  "name": "interest-protocol",
  "version": "1.0.0",
  "description": "Interest Protocol contracts",
  "author": "DeFi, Lda <jose@interestprotocol.com>",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://interestprotocol.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/interest-protocol/v1-core.git"
  },
  "keywords": [
    "DeFi",
    "solidity",
    "contracts",
    "ethereum"
  ],
  "files": [
    "/contracts/**/*.sol",
    "!/contracts/mock/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts .",
    "type-check": "tsc --pretty --noEmit",
    "clean": "yarn lint && yarn type-check && yarn solhint",
    "solhint": "solhint contracts/**/*.sol",
    "test": "npx hardhat test",
    "compile": "npx hardhat compile",
    "coverage": "npx hardhat coverage --testfiles './test/**.ts'",
    "publish": "yarn publish contracts --access public",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.8.0",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.0",
    "pinst": "^3.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.16",
    "ts-node": "^10.1.0",
    "typechain": "^7.0.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.5.0",
    "@openzeppelin/contracts-upgradeable": "^4.5.1",
    "@openzeppelin/hardhat-upgrades": "^1.15.0"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "packageManager": "yarn@3.2.0"
}
